{
	"manifest_version":2,
	"name":"DBTest",
	"version":"0.0.2",
	"author":"me",
	"description":"Плагин для работы с тестами по БД",
	"browser_action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"permissions": [
		"tabs"
	]
}